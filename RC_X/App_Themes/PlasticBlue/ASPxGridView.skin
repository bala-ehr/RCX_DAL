<%@ Register TagPrefix="dx" Namespace="DevExpress.Data" Assembly="DevExpress.Data.v17.2, Version=17.2.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>
<%@ Register TagPrefix="dx" Namespace="DevExpress.Web" Assembly="DevExpress.Web.v17.2, Version=17.2.4.0, Culture=neutral, PublicKeyToken=b88d1754d700e49a" %>

<dx:ASPxGridView runat="server" CssFilePath="{0}/styles.css" CssPostfix="PlasticBlue" >
    <Images SpriteCssFilePath="{0}/sprite.css">
    </Images>

    <Styles CssPostfix="PlasticBlue" CssFilePath="{0}/styles.css">
        <Header SortingImageSpacing="10px" ImageSpacing="10px"></Header>
    </Styles>

    <SettingsPager ShowDefaultImages="False" CurrentPageNumberFormat="{0}">
        <AllButton Text="All"></AllButton>
        <NextPageButton ShowDefaultText="True" TextFormatString="{0} &gt;"></NextPageButton>
        <PrevPageButton ShowDefaultText="True" TextFormatString="&lt; {0}"></PrevPageButton>
    </SettingsPager>
    <SettingsPopup>
        <CustomizationWindow MinWidth="200px" MinHeight="250px"  />
    </SettingsPopup>
</dx:ASPxGridView>

