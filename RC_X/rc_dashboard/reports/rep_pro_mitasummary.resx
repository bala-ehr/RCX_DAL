  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT DISTINCT 
                         TOP (100) PERCENT rc_scorecards_scores.score_col1 AS [Score Column 1], rc_scorecards_scores.score_colmeasure1 AS [Measure Column 1], rc_scorecards_scores.score_col2 AS [Score Column 2], 
                         rc_scorecards_scores.score_colmeasure2 AS [Measure Columns 2 ], rc_scorecards_standards.stan_standard AS Standard, rc_scorecards_standards.stan_capabilityquality AS [Standard Capability Quality], 
                         rc_scorestandard_values.value_score AS [Score #], rc_scorestandard_values.value_scoretext AS [Score Definition], rc_scorestandard_values.value_measures AS [ignore.0], rc_scorecards.card_name AS [Card Name], 
                         rc_scorecards.card_measure AS [ignore.1], rc_scorecards.card_measure_notes AS [ignore.2], rc_scorecards.card_desc AS [ignore.3], rc_scorecards.card_col1 AS [Milestone 1 Label], rc_scorecards.card_measure1, 
                         rc_scorecards.card_colenabled1, rc_scorecards.card_col2 AS [Milestone 2 Label], rc_scorecards.card_measure2, rc_assessment_processes.bp_name AS [Business Process Name], 
                         rc_assessment_processes.bp_identifier AS [Business Process ID], rc_assessment_areas.area_name AS [area name for BA], rc_assessment_areas.area_identifier AS [area ID for BA], 
                         rc_assessment_areas_1.area_name AS [area name for BP], rc_assessment_areas_1.area_identifier AS [area ID for BP], dbo.rc_projects.pro_name AS [Project Name], rc_scorecards_scores.score_itemid, 
                         rc_assessment_areas.area_id, rc_assessment_processes.bp_id, rc_assessment_processes.bp_description, rc_assessment_areas.area_description, dbo.rc_projects.pro_ID
FROM            dbo.rc_projects RIGHT OUTER JOIN
                         dbo.rc_scorecards_scores AS rc_scorecards_scores INNER JOIN
                         dbo.rc_scorecards_standards AS rc_scorecards_standards ON rc_scorecards_standards.stan_id = rc_scorecards_scores.score_standard INNER JOIN
                         dbo.rc_scorestandard_values AS rc_scorestandard_values ON rc_scorestandard_values.value_standard = rc_scorecards_standards.stan_id INNER JOIN
                         dbo.rc_scorecards AS rc_scorecards ON rc_scorecards.card_id = rc_scorecards_standards.stan_card ON dbo.rc_projects.pro_ID = rc_scorecards_scores.score_projectid LEFT OUTER JOIN
                         dbo.rc_assessment_active INNER JOIN
                         dbo.rc_assessment_processes AS rc_assessment_processes ON dbo.rc_assessment_active.act_bpID = rc_assessment_processes.bp_id ON 
                         rc_scorecards_scores.score_itemid = rc_assessment_processes.bp_id LEFT OUTER JOIN
                         dbo.rc_assessment_categories AS rc_assessment_categories ON rc_assessment_categories.cat_id = rc_assessment_processes.bp_bcID LEFT OUTER JOIN
                         dbo.rc_assessment_areas AS rc_assessment_areas ON rc_assessment_areas.area_id = rc_scorecards_scores.score_itemid LEFT OUTER JOIN
                         dbo.rc_assessment_areas AS rc_assessment_areas_1 ON rc_assessment_areas_1.area_id = rc_assessment_categories.cat_baID
WHERE        (rc_scorecards.card_name &lt;&gt; N'Barbara BCM Test') AND (dbo.rc_projects.pro_name IS NOT NULL) AND (rc_scorecards.card_name &lt;&gt; N'TEST CM') AND (rc_scorecards.card_name &lt;&gt; N'ICM Scorecard')
ORDER BY [Card Name]</value>
